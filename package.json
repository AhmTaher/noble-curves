    "./pasta": {
      "types": "./pasta.d.ts",
      "default": "./pasta.js"
    },
    "./secp256k1": {
      "types": "./secp256k1.d.ts",
      "default": "./secp256k1.js"
    }
  },
  "keywords": [
    "elliptic",
    "curve",
    "cryptography",
    "weierstrass",
    "montgomery",
    "edwards",
    "p256",
    "p384",
    "p521",
    "secp256r1",
    "secp256k1",
    "ed25519",
